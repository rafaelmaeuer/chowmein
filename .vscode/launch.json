    "version": "0.2.0",
    "configurations": [
        {
            "name": "Lable Topic",
            "type": "python",
            "request": "launch",
            "program": "./label_topic.py",
            "console": "integratedTerminal",
            "args": [
                "--line_corpus_path", "./datasets/corona-short.dat",
                "--preprocessing", "wordlen", "tag",
                "--label_tags", "NN,NN", "JJ,NN", "ADJA,NN", "FM,NN", "NE,NE", "JJ,NE", "ADJA,NE", "FM,NE", "NN,NE", "NE,NN"//['NN,NN', 'JJ,NN', 'NN,NE', 'JJ,NE', 'NE,NE', 'ADJA,NN', 'ADJA,NE', 'JJ,ADJA']
            ],
            "env": {
                "NLTK_DATA": "./nltk_data"
            }
        }
    ]
}